{
  "name": "foldkit",
  "version": "0.15.0-canary.0",
  "description": "Elm-inspired UI framework powered by Effect",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./html": {
      "types": "./dist/html.d.ts",
      "import": "./dist/html.js"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.js"
    },
    "./route": {
      "types": "./dist/route.d.ts",
      "import": "./dist/route.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    },
    "./fieldValidation": {
      "types": "./dist/fieldValidation.d.ts",
      "import": "./dist/fieldValidation.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./struct": {
      "types": "./dist/struct.d.ts",
      "import": "./dist/struct.js"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "import": "./dist/url.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "pnpm run clean && tsc -b",
    "watch": "tsc -b --watch",
    "typecheck": "tsc -b --noEmit",
    "test": "vitest run"
  },
  "peerDependencies": {
    "effect": "^3.18.2"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "effect": "^3.19.14",
    "happy-dom": "^18.0.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "effect",
    "ui",
    "elm",
    "functional",
    "typescript"
  ],
  "author": "Devin Jameson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devinjameson/foldkit.git",
    "directory": "packages/foldkit"
  },
  "dependencies": {
    "@effect/platform-browser": "^0.74.0",
    "snabbdom": "^3.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
